            #if($auth.hasPermission("$token","system:menu:newOneMenu"))
                <button type="button" onclick="insertFirstMenu_btn()" class="btn btn-primary">新建一级菜单</button>
            #end
        </div>
    </div>
    <div class="content-table">
        <div class="wode">
            <div class="box-body">
                <table id="menuList"></table>
            </div>
        </div>
    </div>
