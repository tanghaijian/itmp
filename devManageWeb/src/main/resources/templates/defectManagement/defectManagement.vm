<div class="color1">
    <div class="container-fluid" id="search_div">
        <div class="row control-row">
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">缺陷编号：</label></div>
                <div class="search_input">
                    <input id="defectStatusList" type="hidden" value="$defectStatusList"/>
                    <input type="text" class="form-control" placeholder="请输入" id="defectCode" name="defectCode" value=""
                           onkeyup="this.value=this.value.replace(/(^\s*)|(\s*$)/g, '')"/>
                </div>
            </div>
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">缺陷摘要：</label></div>
                <div class="search_input">
                    <input type="text" class="form-control" placeholder="请输入" id="defectSummary" name="defectSummary"
                           value=""/>
                </div>
            </div>
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">提出人：</label></div>
                <div class="search_input">
                    <input id="submitUserId" type="hidden">
                    <input type="text" class="form-control pointStyle" onFocus="this.blur();" placeholder="请选择"
                           id="submitUserName" name="submitUserName"/>
                </div>
            </div>
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">提出日期：</label></div>
                <div class="search_input">
                    <input size="16" type="text" onFocus="this.blur();"
                           class="form-control  clear form_datetime pointStyle" placeholder="请选择日期" id="submitDate"
                           name="submitDate"/>
                </div>
            </div>
        </div>

        <div class="row control-row">
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">缺陷状态：</label></div>
                <div class="search_input">
                    <select class="selectpicker" id="defectStatus" title="请选择" multiple>
                        #foreach($dictMap in ${dict.getDictMap(
                            "TBL_DEFECT_INFO_DEFECT_STATUS").entrySet()})
                            <option value="${dictMap.key}">${dictMap.value}</option>
                        #end
                    </select>
                </div>
            </div>
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">缺陷来源：</label></div>
                <div class="search_input">
                    <select class="selectpicker" id="defectSource" title="请选择" multiple>
                        #foreach($dictMap in ${dict.getDictMap(
                            "TBL_DEFECT_INFO_DEFECT_SOURCE").entrySet()})
                            <option value="${dictMap.key}">${dictMap.value}</option>
                        #end
                    </select>
                </div>
            </div>
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">严重级别：</label></div>
                <div class="search_input">
                    <select class="selectpicker" id="severityLevel" title="请选择" multiple>
                        #foreach($dictMap in ${dict.getDictMap(
                            "TBL_DEFECT_INFO_SEVERITY_LEVEL").entrySet()})
                            <option value="${dictMap.key}">${dictMap.value}</option>
                        #end
                    </select>
                </div>
            </div>
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">紧急程度：</label></div>
                <div class="search_input">
                    <select class="selectpicker" id="emergencyLevel" title="请选择" multiple>
                        #foreach($dictMap in ${dict.getDictMap(
                            "TBL_DEFECT_INFO_EMERGENCY_LEVEL").entrySet()})
                            <option value="${dictMap.key}">${dictMap.value}</option>
                        #end
                    </select>
                </div>
            </div>
        </div>

        <div class="row control-row">
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">投产窗口：</label></div>
                <div class="search_input">
                    <input id="windowId" type="hidden"/>
                    <input type="text" class="form-control pointStyle" onFocus="this.blur();" placeholder="请选择"
                           id="windowName" name="windowName"/>
                </div>
            </div>
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">所属需求：</label></div>
                <div class="search_input">
                    <input type="hidden" id="requirementCode"/>
                    <input type="text" class="form-control pointStyle" onFocus="this.blur();" placeholder="请选择"
                           id="requirementName" name="requirementName"/>
                </div>
            </div>
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">涉及子系统：</label></div>
                <div class="search_input">
                    <input id="sel_systemId" type="hidden">
                    <input type="text" class="form-control pointStyle" onFocus="this.blur();" placeholder="请选择"
                           id="systemName" name="systemName"/>
                </div>
            </div>
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">缺陷类型：</label></div>
                <div class="search_input">
                    <select class="selectpicker" id="defectType" title="请选择" multiple>
                        #foreach($dictMap in ${dict.getDictMap(
                            "TBL_DEFECT_INFO_DEFECT_TYPE").entrySet()})
                            <option value="${dictMap.key}">${dictMap.value}</option>
                        #end
                    </select>
                </div>
            </div>
        </div>
        <div class="row control-row">
            <div class="col-md-3">
                <div class="search_font"><label class="control-label">主修复人：</label></div>
                <div class="search_input chanceMyself">
                    <input id="sel_assingUserId" type="hidden">
                    <input type="text" class="form-control def_col_20 pointStyle" onFocus="this.blur();"
                           placeholder="请选择" id="sel_assingUserName" name="assingUserName"/>
                    <div class="def_col_16">
                        <button type="button" class="btn btn-primary chanceMyselfBtm"
                                onclick="selectMyselfDefect(this)">选自己
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <div class="row control-row search_btn">
            <button type="button" class="btn btn-primary" onclick="searchInfo()">搜&nbsp;&nbsp;&nbsp;索</button>
            <button type="button" onclick="reset()" class="btn btn-default">重&nbsp;&nbsp;&nbsp;置</button>
        	<div class="collection">
					<span class="fa fa-heart-o"></span>&nbsp;收藏此次筛选
			</div>
			<select id="projectType2" class="selectpicker form-control" onchange="tab_option(this)">
						<option value="">选择收藏方案</option>
					</select>
        </div>
       
    </div>
    <div id="downBtn" class="downBtn">
        <span class="fa fa-caret-up"></span>
    </div>
</div>
<div class="main-content">
    <div class="content-btn">
        #if($auth.hasPermission("$token","devManage:defect:export"))
            <button type="button" onclick="exportDefect_btn()" class="btn btn-default">导出缺陷</button>&nbsp;
        #end
    </div>
    <div class="content-table">
    	<div class="wode" id="list_body2">
            <table id="list2"></table>
            <div id="pager2"></div>
        </div>
        <div class="wode"  id="list_body" style="display:none;">
            <table id="defectList"></table>
            <div id="defectListPager"></div>
        </div>
        
    </div>
</div>


