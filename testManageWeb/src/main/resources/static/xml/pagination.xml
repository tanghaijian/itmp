<!-- pagination --> 
<_PAGE_ id="pagination"><![CDATA[ 
<div class="row" style="background-color:#D2E7F7;margin:-20px 0px;padding-top:6px;">
	<div class="col-sm-6" style="width: 70%;">
		 <label style="float: left;">
				<select  name="pageSize"  style="height: 33px;">
					<option value="10">10</option>
					<option value="20">20</option>
					<option value="50">50</option>
					<option value="100">100</option>
					<option value="200">200</option>
				</select>
		 </label>
		 <ul class="pagination" style="float: left;margin: 0px 6px;">
			<li>
				<a name="page_first" href="#">首页</a>
			</li>
			<li>
				<a name="page_prev"  href="#">前一页</a>
			</li>
			<li>
				<a name="page_next"  href="#">后一页</a>
			</li>
			<li>
				<a name="page_last"  href="#">末页</a>	
			</li>
		</ul>
		<div>
			<span>当前页，第</span>
			<span name="pageIndex">1</span>
			<span>页，总共<span name="totalPage">0</span>页。</span>
